<h1>Listing tipo_residuos</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tipo_residuos.each do |tipo_residuo| %>
      <tr>
        <td><%= tipo_residuo.nombre %></td>
        <td><%= link_to 'Show', tipo_residuo %></td>
        <td><%= link_to 'Edit', edit_tipo_residuo_path(tipo_residuo) %></td>
        <td><%= link_to 'Destroy', tipo_residuo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tipo residuo', new_tipo_residuo_path %>
